<!-- this template displays a list of matches.
usage: > songs-template
-->
<div class="container">
  <div class="row">
    <div class="col-xs-12 col-md-12 col-lg-12">
      <table class="table table-hover">
        <thead class="thead-light">
        <col width="50%">
        <col width="50%">
        <tr>
          <th scope="col">Artist</th>
          <th scope="col">Song</th>
        </tr>
        </thead>
        <tbody>
        {{#each songs}}
        <tr>
          <td>{{artist}}</td>
          <td>{{title}}</td>
        </tr>
        {{/each}}
        <tbody>
      </table>
    </div>
  </div> <!-- / div row -->
</div>
